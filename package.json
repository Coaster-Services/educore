	"dependencies": {
		"@aws-sdk/client-s3": "^3.556.0",
		"@coasterfan5/ui": "^0.0.14",
		"@fontsource/open-sauce-one": "^5.0.11",
		"@iconify-json/ph": "^1.1.12",
		"@leodog896/bodyguard": "^1.3.2",
		"@prisma/client": "5.12.1",
		"prisma": "^5.12.1",
		"tsx": "^4.7.2",
		"unplugin-icons": "^0.18.5",
		"zod": "^3.23.4",
		"zod-validation-error": "^3.2.0"
	}
}
